{
    "openapi": "3.0.0",
    "info": {
        "title": "API de Gesti칩n de EPS",
        "description": "Endpoints para manejar informaci칩n de EPS y documentos asociados.",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8000/api"
        }
    ],
    "paths": {
        "/aspirante/crear-eps": {
            "post": {
                "tags": [
                    "EPS"
                ],
                "summary": "Crear EPS",
                "description": "Crea una nueva EPS y sube un archivo asociado (PDF, JPG, PNG). Requiere autenticaci칩n.",
                "operationId": "dfd12d26544ca58eb4c6a89d7bbc0707",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "required": [
                                    "nombre_eps",
                                    "tipo_afiliacion",
                                    "estado_afiliacion",
                                    "fecha_afiliacion_efectiva",
                                    "tipo_afiliado",
                                    "archivo"
                                ],
                                "properties": {
                                    "nombre_eps": {
                                        "type": "string",
                                        "maxLength": 100,
                                        "minLength": 7,
                                        "example": "Salud Total EPS"
                                    },
                                    "tipo_afiliacion": {
                                        "type": "string",
                                        "enum": [
                                            "contributivo",
                                            "subsidiado",
                                            "especial"
